{
  "name": "dukeitout",
  "version": "1.0.0",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "npm run build-client && node server",
    "client": "cd client && yarn start",
    "server": "node server.js",
    "build-client": "cd client && npm run build && cd ..",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "express": "^4.16.4",
    "mysql": "^2.16.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "^2.1.3",
    "reactstrap": "^7.1.0",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
